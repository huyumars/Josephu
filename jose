#include <iostream>
using namespace std;
typedef int Node_entry;
struct Node {
	Node_entry entry;
	Node *next;
	Node();
	Node(Node_entry item, Node *add_on = NULL);
};
class CirList {
public:
	CirList();
	void create_CirList(int n);
	void add_from_head(Node_entry item);//头插
	void add_from_tail(Node_entry item);//尾插
	void print();
	Node* del(int m,Node*p);
	Node* get(Node* node, int m);
	int length();
	Node* get_Head();
	void list_clear();
private:
	Node* head;
	Node* tail;
	int count;
};
Node::Node() {
	next = NULL;
}
Node::Node(Node_entry item, Node*add_on)
{
	entry = item;
	next = add_on;
}

CirList::CirList() {
	head = tail = NULL;
	count = 0;
}
void CirList::create_CirList(int n) {
	Node *s = new Node;
	head =s;
	head->entry = 1;
	for (int i = 2;i <=n;i++) {
		Node*w = new Node;
		w->entry = i;
		s->next = w;
		s = w;
	}
	tail = s;
	tail->next = head;
	count = n;
}
void CirList::add_from_head(Node_entry item) {
	Node*p=new Node;
	p->next=head;
	head=p;
	tail->next=head;
}
void CirList::add_from_tail(Node_entry item) {
	Node*p=new Node;
	tail->next=p;
	tail=p;
	tail->next=head;
}
void CirList::print() {
	Node*p;
	for (p = head;p != tail;p=p->next) {
		cout << p->entry;
	}
	cout<<tail->entry;
}
Node*CirList::del(int m,Node*p) {
	Node*q;
	int j = 1;//计数器
	while (j != m-1) {
		j++;
		p=p->next;
	}
	q = p;
	p = p->next;
	if (p == head) {
		head = p->next;
		tail->next = head;
		delete p;
		return head;
	}
	else if (p == tail) {
		q->next = head;
		tail = q;
		delete p;
		return head;
	}
	else {
		q->next = p->next;
		delete p;
		return q->next;
	}
}
Node* CirList::get(Node* node, int m) {
	Node*p;#include "Class.h"
#include<iostream>
using namespace std;
Node::Node() {
	next = nullptr;
}
Node::Node(Node_entry item, Node*add_on)
{
	entry = item;
	next = add_on;
}

	p = node;
	for (int i = 0;i < m - 1;i++) {
		p = p->next;
	}
	return p;
}
int CirList::length() {
	return count;
}
Node* CirList::get_Head() {
	return head;
}
void CirList::list_clear() {
	Node*p=head;
	Node*q;
	for(int i=0;i<count;i++){
        q=p;
        if(p!=tail){
            p=p->next;
        }
        delete[]q;
	}
	count = 0;
}
int main() {
	cout << "Please enter n,m:" << endl;
	int m, n;
	cin >> n >> m;
	Node*p=new Node;
	CirList cirlist;
	cirlist.create_CirList(n);
	p=cirlist.get_Head();
	int count=cirlist.length();
	while(count>1){
        p=cirlist.del(m,p);
        cirlist.print();
        cout<<endl;
        count--;
	}
	cirlist.print();
	return 0;
}
